{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "target": "es2017",
    "sourceMap": true,
    "incremental": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "importHelpers": true
  },
}
