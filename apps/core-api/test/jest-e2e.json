{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/shared": "<rootDir>/src/shared/index",
    "@app/test": "<rootDir>/test/index",
    "@app/admin-users": "<rootDir>/src/users/admin-users/index"
  }
}
