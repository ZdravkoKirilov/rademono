{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../src",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/shared": "<rootDir>/shared/index",
    "@app/admin-users": "<rootDir>/users/admin-users/index"
  }
}
