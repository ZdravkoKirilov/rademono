{
  "name": "clients",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "test": "jest projects",
    "test:watch": "jest projects --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:games-admin": "ng serve --project=games-admin --port 4000",
    "start:showcases": "ng serve --project=showcases --port 4001",
    "start:apps": "npm run start:games-admin & npm run start:showcases",
    "build:ui:watch": "ng build --project=ui --watch",
    "build:ui": "ng build --project=ui",
    "build:render-kit:watch": "ng build --project=render-kit --watch",
    "start": "run-s build:ui start:apps",
    "generate-icons": "svg-generator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.6",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/forms": "~11.2.6",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/router": "~11.2.6",
    "@ngneat/svg-icon": "^3.1.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "fp-ts": "^2.9.5",
    "ngx-quill": "^13.2.0",
    "normalize.css": "^8.0.1",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@ngneat/svg-generator": "^2.0.0",
    "@testing-library/angular": "^10.4.1",
    "@types/jest": "^26.0.21",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "ng-packagr": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  },
  "svgGenerator": {
    "outputPath": "./projects/ui/src/generated-icons",
    "prefix": "app",
    "srcPath": "./projects/ui/src/assets/svg",
    "svgoConfig": {
      "plugins": [
        {
          "removeDimensions": true,
          "cleanupAttrs": true
        }
      ]
    }
  }
}
